{
  "author": "Faizan Durrani @FaizanDurrani",
  "bin": {
    "paperback-cli": "./bin/run.js"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@oclif/core": "^3.27.0",
    "@oclif/plugin-help": "^6.2.16",
    "@oclif/plugin-plugins": "^5.4.15",
    "@paperback/runtime-polyfills": "*",
    "@paperback/types": "*",
    "@protobuf-ts/runtime-rpc": "^2.9.4",
    "@types/fs-extra": "^11.0.4",
    "buffer": "^6.0.3",
    "chalk": "^4",
    "esbuild": "^0.19.12",
    "fs-extra": "^11.2.0",
    "listr2": "^8.2.5"
  },
  "description": "The Paperback extension development toolchain",
  "devDependencies": {
    "@oclif/test": "^3.2.15",
    "@protobuf-ts/plugin": "^2.9.4",
    "@protobuf-ts/protoc": "^2.9.4",
    "@types/chai": "^4.3.20",
    "@types/ip": "^1.1.3",
    "@types/listr": "^0.14.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.19.59",
    "chai": "^4.5.0",
    "grpc-tools": "^1.12.4",
    "mocha": "^10.7.3",
    "oclif": "^4.15.12",
    "prettier": "^3.3.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/FaizanDurrani/paperback-toolchain",
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@paperback/toolchain",
  "oclif": {
    "bin": "paperback-cli",
    "dirname": "paperback-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "FaizanDurrani/paperback-toolchain",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b && cp -r src/homepage dist/ && cp -r src/shims dist/",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "1.0.0-alpha.23",
  "bugs": "https://github.com/FaizanDurrani/paperback-toolchain/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts",
  "gitHead": "f7fcc07ceb1cf80f903abb7b63db9fbec01ab690"
}
